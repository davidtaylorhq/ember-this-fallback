{
  "name": "ember-this-fallback",
  "version": "0.0.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build:ts": "cd lib && tsc",
    "build": "yarn build:ts && ember build --environment=production",
    "clean": "cd lib && tsc --build --clean",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "start": "yarn build:ts && ember serve && yarn clean",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "yarn build:ts && ember test && yarn clean",
    "test:ember-compatibility": "yarn build:ts && ember try:each",
    "prepack": "yarn build:ts && ember ts:precompile",
    "postpack": "yarn clean && ember ts:clean"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-cli-typescript": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.0.1",
    "@ember/test-helpers": "^2.9.3",
    "@embroider/test-setup": "^2.1.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/syntax": "^0.84.3",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "^1.0.0-beta.4",
    "@glint/environment-ember-loose": "^1.0.0-beta.4",
    "@glint/template": "^1.0.0-beta.4",
    "@tsconfig/ember": "^2.0.0",
    "@types/ember-qunit": "^6.1.1",
    "@types/qunit": "^2.19.4",
    "@types/rsvp": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.0.1",
    "ember-auto-import": "^2.6.3",
    "ember-cli": "~4.11.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^2.1.2",
    "ember-modifier": "^4.1.0",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^6.2.0",
    "ember-resolver": "^10.0.0",
    "ember-source": "~4.11.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.7.2",
    "ember-try": "^2.0.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-ember": "^11.5.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-qunit": "^7.3.4",
    "eslint-plugin-unicorn": "^46.0.0",
    "loader.js": "^4.7.0",
    "prettier": "^2.8.7",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "typescript": "^5.0.4",
    "webpack": "^5.78.0"
  },
  "peerDependencies": {
    "ember-source": "^3.28.0 || ^4.0.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
